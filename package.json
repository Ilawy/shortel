{
  "name": "url-shortener",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:backend": "cd packages/backend && npm run start:dev",
    "start:frontend": "cd packages/frontend && npm run start",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
